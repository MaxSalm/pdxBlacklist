  issue           = {12},
  keywords        = {Algorithms; Animals; DNA, Complementary, genetics; Genome, genetics; Humans; Mice; Sequence Analysis, RNA, methods; Software; Transplantation, Heterologous, classification},
  nlm             = {PMC3371868},
